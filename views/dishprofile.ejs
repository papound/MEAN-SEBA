    <script type="text/ng-template" id="tabDialog.tmpl.html">
        <md-dialog aria-label="{[{ dishName }]}">
            <form>
                <md-toolbar>
                    <div class="md-toolbar-tools">
                        <h2 ng-model="dish_name">{[{ dishName }]}</h2>
                        <span flex></span>
                        <md-button class="md-icon-button" ng-click="cancel()">
                            <md-icon md-svg-src="img/icons/ic_close_24px.svg" aria-label="Close dialog"></md-icon>
                        </md-button>
                    </div>
                </md-toolbar>
                <md-dialog-content style="max-width:800px;max-height:810px; ">
                    <md-tabs md-dynamic-height md-border-bottom>
                        <md-tab label="Nutrition facts">
                            <md-content class="md-padding">
                                <h1 class="md-display-2"></h1>
                                <table class="table">
                                    <tr>
                                        <td><b style="color: red">Calories : </b>{[{ dishNutritions[0].Calories }]}</td>
                                        <td><b style="color: #fdd835">Carbohydrates : </b>{[{
                                            dishNutritions[0].Carbohydrates }]}
                                        </td>
                                    </tr>
                                    <tr>
                                        <td><b style="color: #673ab7">Cholesterols : </b>{[{
                                            dishNutritions[0].Cholesterols }]}
                                        </td>
                                        <td><b style="color: #d58512">Fats : </b>{[{ dishNutritions[0].Fats }]}</td>
                                    </tr>
                                    <tr>
                                        <td><b style="color: #0b8043">Fiber : </b>{[{ dishNutritions[0].Fiber }]}</td>
                                        <td><b style="color: #8a6d3b">Proteins : </b>{[{ dishNutritions[0].Proteins }]}
                                        </td>
                                    </tr>
                                    <tr>
                                        <td><b style="color: darkblue">Sodium : </b>{[{ dishNutritions[0].Sodium }]}
                                        </td>
                                        <td></td>
                                    </tr>
                                </table>
                            </md-content>
                        </md-tab>
                        <md-tab label="Ingredients">
                            <md-content class="md-padding">
                                <!--<h1 class="md-display-2">Ingredients</h1>-->
                                <table class="table table-hover">
                                    <thead>
                                    <tr>
                                        <th width="20%" style="color: #0b50bf">Quantity</th>
                                        <th width="20%" style="color: #9e5406">Unit</th>
                                        <th width="40%" style="color: forestgreen">Ingredient</th>
                                        <th width="20%" style="color: steelblue">+ Top up</th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    <tr ng-repeat="di in dishIngredients">
                                        <td>{[{ di.amount }]}</td>
                                        <td>{[{ di.unit }]}</td>
                                        <td>
                                            <b>{[{ di.name }]}</b>
                                            &nbsp;&nbsp;<i>{[{ di.detail }]}</i>
                                        </td>
                                        <td>
                                            <md-input-container class="md-block small_number">
                                                <input type="number" step="any" min="0" max="999"/>
                                            </md-input-container>
                                        </td>
                                    </tr>
                                    </tbody>
                                </table>
                            </md-content>
                        </md-tab>
                        <md-tab label="Recipe">
                            <md-content class="md-padding">
