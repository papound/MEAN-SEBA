        var firstName = fullName.split(' ').slice(0, -1).join(' ');
        var lastName = fullName.split(' ').slice(-1).join(' ');
        var email = profile.getEmail();
        var image = profile.getImageUrl();
        $('a#login').text(firstName);
        //$('a#login').removeAttr("onclick");
        $('a#login').removeAttr('href');
        $('a#login_collapse').text("Welcome! " + firstName);
        $('a#login_collapse').removeAttr("onclick");
        $('a#login_collapse').removeAttr('href');
        $('#login_icon').addClass('login_icon2').removeClass('login_icon');
        var url = "http://localhost:4000/main-profile";
        //localStorage.registerChefAtHome = 1
        console.log('localStorageregisterChefAtHome: '+ localStorage.registerChefAtHome)
        if (!localStorage.registerChefAtHome) {
            localStorage.registerChefAtHome = 1;
            $.ajax({
                url : "http://localhost:4000/api/register/google",
                type: "POST",
                data: JSON.stringify({email: email}),
                contentType: "application/json; charset=utf-8",
                dataType   : "json",
                success    : function(){
                    localStorage.loginChefAtHomeEmail = email;
                    $('a#login').attr('href', url);
                    //alert("Success: "+ email);
                    //$("#position").hide();
                }
            });
        }b

        if (typeof(Storage) !== "undefined") {
            //localStorage.visitProfile = 1;
            if (localStorage.loginChefAtHomeEmail) {
                if (localStorage.loginChefAtHomeEmail != email && localStorage.visitProfile >= 1) {
                    //login on this pc once but on different user
                    localStorage.loginChefAtHomeEmail = email;
                    localStorage.loginChefAtHomefullname = fullName;
                    localStorage.loginChefAtHomefirstname = firstName;
                    localStorage.loginChefAtHomelastname = lastName;
                    localStorage.loginChefAtHomeimage = image;
                } else {
                    //detect same user
                    console.log("Welcome Back! " + firstName)
                    localStorage.loginChefAtHomeEmail = email;
                    localStorage.loginChefAtHomefullname = fullName;
                    localStorage.loginChefAtHomefirstname = firstName;
                    localStorage.loginChefAtHomelastname = lastName;
                    localStorage.loginChefAtHomeimage = image;
                    //$('a#login').attr('href', url);
                }
            } else {
                //Never login on this pc before
                localStorage.loginChefAtHomefullname = fullName;
                localStorage.loginChefAtHomefirstname = firstName;
                localStorage.loginChefAtHomelastname = lastName;
                localStorage.loginChefAtHomeimage = image;
                localStorage.loginChefAtHomeEmail = email;
                localStorage.visitProfile = 1;
                //window.location.href = "http://localhost:4000/main-profile";
            }
            console.log('Logged in as: ' + googleUser.getBasicProfile().getName());
