            <!-- Edit profile-->
            <form action="" method="post" name="projectform">
                <div class="row">

                    <!-- Left side column-->
                    <div class="col-md-3">
                        <!--Space for uploading profilie picture-->
                    </div>
                    <!-- End left side column-->

                    <!--Main column-->
                    <div class="col-md-4">
                        <!-- Get name from log in-->
