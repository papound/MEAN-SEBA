                            <div class="col-md-3">
                                DateOfBirth
                            </div>
                            <div ng-controller="DateCtrl" style='padding: 40px;' ng-cloak>
                                <md-datepicker ng-model="myDate" md-placeholder="Enter date"></md-datepicker>
                            </div>

                        </div>


                        <!--Height-->
                        <div class="row">
                            <div class="col-md-4">
                                Height
                            </div>
                            <div class="col-md-8" ng-controller="ErrCtrl">
                                <!--<input type="number" name="myHeight" value="165" style="width: 100px">-->
                                <md-input-container class="md-block" flex-gt-sm>
                                    <label>Height</label>
                                    <input name="height" type="number" ng-pattern="/^[0-9]{1,7}$/" required
                                           ng-model="project.rate" min="1" max="999">
                                    <div ng-messages="projectform.height.$error" multiple md-auto-hide="false">
                                        <div ng-message="required">
                                            You've to type something!
                                        </div>
                                        <div ng-message="pattern">
                                            You type wrong pattern!
                                        </div>
                                        <div ng-message="min">
                                            Your're the shortest one in earth!
                                        </div>
                                        <div ng-message="max">
                                            You're the tallest person in the Earth!
                                        </div>
                                    </div>
                                </md-input-container>
                            </div>
                        </div>
                        <br>

                        <!--Weight-->
                        <div class="row">
                            <div class="col-md-4">
                                Weight
                            </div>
                            <div class="col-md-8" ng-controller="ErrCtrl">
                                <!--<input type="number" name="myWeight" value="65" style="width: 100px">-->
                                <md-input-container class="md-block" flex-gt-sm>
                                    <label>Weight</label>
                                    <input name="weight" type="number" ng-pattern="/^[0-9]{1,7}$/" required
                                           ng-model="project.rate" min="1" max="999">
                                    <div ng-messages="projectform.weight.$error" multiple md-auto-hide="false">
                                        <div ng-message="required">
                                            You've to type something!
                                        </div>
                                        <div ng-message="pattern">
                                            You type wrong pattern!
                                        </div>
                                        <div ng-message="min">
                                            Your weight is less than any human in earth!
                                        </div>
                                        <div ng-message="max">
                                            Your weight is highest among every human in earth!
                                        </div>
                                    </div>
                                </md-input-container>
                            </div>
                        </div>
                        <br>

                        <!--Vegetarian-->
                        <div class="row">
                            <div class="col-md-4">
                                Vegetarian
                            </div>
                            <div class="col-md-8" ng-controller="Chk1Ctrl">
                                <md-checkbox ng-model="data.cb1" aria-label="">
                                    {[{ data.cb1 }]}
                                </md-checkbox>
                            </div>
                        </div>
                        <br>

                        <!-- Not preferring-->
                        <div class="row">
                            <div class="col-md-4">
                                Do
                                not
                                prefer:
                            </div>
                            <div class="col-md-8">
                                <div class="row">
                                    <div class="col-md-8" ng-controller="Chk1Ctrl">
                                        <md-checkbox ng-model="data.cb2" aria-label="">
                                            Diary Product<span ng-if="data.cb2"> : {[{ data.cb2 }]}</span>
                                        </md-checkbox>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-8" ng-controller="Chk1Ctrl">
                                        <md-checkbox ng-model="data.cb3" aria-label="">
                                            Seafood<span ng-if="data.cb3"> : {[{ data.cb3 }]}</span>
                                        </md-checkbox>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-8" ng-controller="Chk1Ctrl">
                                        <md-checkbox ng-model="data.cb4" aria-label="">
                                        </md-checkbox>
                                        <span ng-if="!data.cb4"> Others</span>
                                        <md-input-container ng-if="data.cb4" ng-disabled="false">
                                            <label>Others, Please Specify</label>
                                            <input ng-model="data.extra" name="other1" required>
                                            <div ng-messages="projectform.other1.$error" multiple md-auto-hide="false">
                                                <div ng-message="required">
                                                    You've to type something!
                                                </div>
                                            </div>
                                        </md-input-container>
                                        <span ng-if="data.cb4"> : {[{ data.extra }]}</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <br>

                        <!--Health Condition-->
                        <h4>Health Condition</h4>
                        <div class="row">
                            <div class="col-md-8" ng-controller="Chk2Ctrl">
                                <md-checkbox ng-model="data2.cb1" aria-label="">
                                    High Blood Pressure<span ng-if="data2.cb1"> : {[{ data2.cb1 }]}</span>
                                </md-checkbox>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-8" ng-controller="Chk2Ctrl">
                                <md-checkbox ng-model="data2.cb2" aria-label="">
                                    Diabetes<span ng-if="data2.cb2"> : {[{ data2.cb2 }]}</span>
                                </md-checkbox>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-8" ng-controller="Chk2Ctrl">
                                <md-checkbox ng-model="data2.cb3" aria-label="">
                                </md-checkbox>
                                <span ng-if="!data2.cb3"> Others</span>
                                <md-input-container ng-if="data2.cb3" ng-disabled="false">
                                    <label>Others, Please Specify</label>
                                    <input ng-model="data2.extra" name="other2" required>
                                    <div ng-messages="projectform.other2.$error" multiple md-auto-hide="false">
                                        <div ng-message="required">
                                            You've to type something!
                                        </div>
                                    </div>
                                </md-input-container>
                                <span ng-if="data2.cb3"> : {[{data2.extra}]}</span>
                            </div>
                        </div>


                    </div>
                    <!-- End main column-->

                    <!--Right side column-->
                    <div class="col-md-5">
                        test3
                    </div>
                    <!--End right side column-->


                </div>
            </form>
        </div>

        <div class="row">
            <div class="col-md-12">


            </div>
        </div>
        <!-- /.container -->
    </div>
</section>
