<script>
    function clickHandler(e) {
        if (!e.target.hasAttribute('data-dialog')) {
            return;
        }
        var id = e.target.getAttribute('data-dialog');
        var dialog = document.getElementById(id);
        if (dialog) {
            dialog.toggle();
            e.target.toggleAttribute && e.target.toggleAttribute('data-dialog-opened', dialog.opened);
        }
    }
    function clearInput() {
        var clear = document.getElementById('inputWithButton');
        clear.value = "";
    }
    function clearInput2() {
        var clear2 = document.getElementById('inputWithButton2');
        clear2.value = "";
    }
</script>

