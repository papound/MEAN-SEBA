<script>
    $(document).ready(function () {
        $("#position").hide();
        var readURL = function (input) {
            if (input.files && input.files[0]) {
                var reader = new FileReader();
                reader.onload = function (e) {
                    $('.profile-pic').attr('src', e.target.result);
                }
                reader.readAsDataURL(input.files[0]);
            }
        }
        $(".file-upload").on('change', function () {
            readURL(this);
        });
        $(".upload-button").on('click', function () {
            $(".file-upload").click();
        });

        if (localStorage.loginChefAtHomeEmail) {
            $('a#login').text(localStorage.loginChefAtHomefirstname);
            $('a#login_collapse').text("Welcome! " + localStorage.loginChefAtHomefirstname);
            $('#login_icon').addClass('login_icon2').removeClass('login_icon');
            setTimeout(checklogin(), 3000);
        } else {
            window.location.href = "http://localhost:4000/";
        }
    });
</script>

