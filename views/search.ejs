<!-- Script for log in-->
<script>

    function loginPopup() {
        if (localStorage.loginChefAtHomeEmail) {
            document.getElementById("afterLogin").open()
        } else {
            document.getElementById("dialog").open()
        }
    }

    function clickHandler(e) {
        if (!e.target.hasAttribute('data-dialog')) {
            return;
        }
        var id = e.target.getAttribute('data-dialog');
        var dialog = document.getElementById(id);
        if (dialog) {
            dialog.toggle();
            e.target.toggleAttribute && e.target.toggleAttribute('data-dialog-opened', dialog.opened);
        }
    }
    function clearInput() {
        var clear = document.getElementById('inputWithButton');
        clear.value = "";
    }
    function clearInput2() {
        var clear2 = document.getElementById('inputWithButton2');
        clear2.value = "";
    }

    function passmatch(e) {
        var password = encodeURIComponent(this.pass);
        var confirmPassword = encodeURIComponent(this.repass);
        if (password != confirmPassword) {
            // do something
        }
    }

